get_outcome <- function(N) {
#Set the colors that exist on the board
color <- rep(c("Black","Red","Green"), c(18,18,2))
#Pass the variable N from the function call to the random sampling
X <- sample( ifelse( color=="Red", 1,-1),  N, replace = TRUE)
S_N <- sum(X)
return(S_N)
}
